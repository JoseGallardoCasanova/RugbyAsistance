{
  "expo": {
    "name": "SquadPro",
    "slug": "squadpro",
    "version": "3.2.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/logo_Old_Green.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563eb"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.squadpro.app",
      "buildNumber": "13",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_Old_Green.png",
        "backgroundColor": "#2563eb"
      },
      "package": "com.squadpro.app",
      "kotlinVersion": "1.9.25"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "19762a08-f396-41e1-b48c-6aa7b066951a"
      }
    }
  }
}
